{
  "compilerOptions": {
    "types": ["@testing-library/jest-dom", "@emotion/jest", "node"],
    "target": "ESNext",
    "module": "ESNext",
    "noImplicitAny": true,
    "lib": ["dom", "esnext"],
    "declaration": false,
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "jsx": "react-jsx",
    "baseUrl": "./"
  }
}
