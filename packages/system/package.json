{
  "name": "@nex-ui/system",
  "version": "0.0.13",
  "private": false,
  "author": "X1ng Yu",
  "description": "A lightweight and performant styling library based on Emotion, focusing on component architecture and developer experience.",
  "scripts": {
    "build": "tsx ../../scripts/build.ts --dts",
