{
  "name": "@nex-ui/styled",
  "version": "0.0.15",
  "author": "X1ng Yu",
  "private": false,
  "description": "Styled API for creating atomic, theme-aware component styling.",
  "keywords": [
    "react",
    "react-component",
    "nui",
    "nex-ui",
    "styled"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rxy001/nex-ui.git",
    "directory": "packages/styled"
  },
  "license": "MIT",
  "scripts": {
