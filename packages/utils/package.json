{
  "name": "@nex-ui/utils",
  "version": "0.0.3",
  "private": false,
  "author": "X1ng Yu",
  "description": "Utility functions for React components.",
  "keywords": [
    "react",
    "react-component",
    "nui",
    "nex-ui",
    "utils"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rxy001/nex-ui.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsx ../../scripts/build.ts --dts",
    "build:fast": "tsx ../../scripts/build.ts",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "lodash.clamp": "4.0.3",
    "lodash.every": "4.6.0",
    "lodash.filter": "4.6.0",
    "lodash.foreach": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.map": "4.6.0",
    "lodash.memoize": "4.1.2",
    "lodash.merge": "4.6.2",
    "lodash.mergewith": "4.6.2",
    "lodash.pickby": "4.6.0",
    "lodash.reduce": "4.6.0",
    "lodash.some": "4.6.0",
    "react-is": "19.1.0"
  },
  "devDependencies": {
    "@types/lodash.clamp": "4.0.9",
    "@types/lodash.every": "4.6.9",
    "@types/lodash.filter": "4.6.9",
    "@types/lodash.foreach": "4.5.9",
    "@types/lodash.get": "4.4.9",
    "@types/lodash.map": "4.6.13",
    "@types/lodash.memoize": "4.1.9",
    "@types/lodash.merge": "4.6.9",
    "@types/lodash.mergewith": "4.6.9",
    "@types/lodash.pickby": "4.6.9",
    "@types/lodash.reduce": "4.6.9",
    "@types/lodash.some": "4.6.9",
    "@types/react-is": "19.0.0"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "sideEffects": false
}
