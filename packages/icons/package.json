{
  "name": "@nex-ui/icons",
  "version": "0.0.6",
  "author": "X1ng Yu",
  "private": false,
  "description": "Shared icons used by NexUI packages.",
  "keywords": [
    "react",
    "react-component",
    "nui",
    "nex-ui",
    "icons"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rxy001/nex-ui.git",
    "directory": "packages/icons"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsx ../../scripts/build.ts --dts",
    "build:fast": "tsx ../../scripts/build.ts",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "sideEffects": false,
  "devDependencies": {
    "@nex-ui/system": "workspace:*",
    "@storybook/react": "8.6.12"
  },
  "dependencies": {
    "@nex-ui/styled": "workspace:*",
    "@nex-ui/utils": "workspace:*"
  }
}
