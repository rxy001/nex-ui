{
  "name": "@nex-ui/monorepo",
  "private": true,
  "version": "0.0.0",
  "author": "X1ng Yu",
  "license": "MIT",
  "scripts": {
    "build": "turbo build -F !@nex-ui/docs -F !@nex-ui/storybook",
    "build:sys-ts": "tsx ./scripts/buildSysTs",
    "build:icons": "tsx ./scripts/buildIcons",
    "build:fast": "turbo build:fast -F !@nex-ui/docs -F !@nex-ui/storybook",
    "build:docs": "turbo build -F @nex-ui/docs",
    "build:sb": "turbo build -F @nex-ui/storybook",
    "dev": "turbo dev",
    "dev:docs": "turbo dev -F @nex-ui/docs",
    "dev:sb": "turbo dev -F @nex-ui/storybook",
    "typecheck": "turbo typecheck -F !@nex-ui/docs",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "test": "jest --config ./jest.config.ts",
    "prettier:check": "prettier --check packages/**/src --cache",
    "prettier:write": "prettier --write packages/**/src --cache",
    "lint": "pnpm lint:pkg && pnpm lint:docs",
    "lint:pkg": "eslint -c ./eslint.config.mjs ./packages",
    "lint:docs": "eslint -c ./docs/eslint.config.mjs ./docs",
    "prepare": "husky"
  },
  "type": "module",
  "devDependencies": {
    "@changesets/changelog-git": "0.2.1",
    "@changesets/cli": "2.27.7",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@emotion/jest": "11.13.0",
    "@eslint/js": "9.24.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-swc": "0.4.0",
    "@swc/core": "1.11.7",
    "@swc/jest": "0.2.37",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/node": "20.14.12",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@types/yargs-parser": "21.0.3",
    "esbuild": "0.24.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-transform-stub": "2.0.0",
    "jest-transformer-svg": "2.1.0",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rollup": "4.17.2",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-preserve-directives": "0.4.0",
    "svgo": "3.3.2",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "turbo": "latest",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "yargs-parser": "21.1.1"
  },
  "packageManager": "pnpm@10.2.0",
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=10.x"
  },
  "overrides": {
    "@testing-library/react": {}
  }
}
